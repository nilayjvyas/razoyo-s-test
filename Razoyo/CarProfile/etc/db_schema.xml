<?xml version="1.0" ?>
<schema xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Setup/Declaration/Schema/etc/schema.xsd">
	<table name="razoyo_carprofile_details" resource="default" engine="innodb" comment="razoyo_carprofile_details Table">
		<column xsi:type="int" name="entity_id" padding="10" unsigned="true" nullable="false" identity="true" comment="Entity Id"/>
		<constraint xsi:type="primary" referenceId="PRIMARY">
			<column name="entity_id"/>
		</constraint>
		<column name="customer_email" nullable="true" xsi:type="varchar" comment="customer_email" length="255"/>
		<column name="car_id" nullable="true" xsi:type="varchar" comment="car_id" length="255"/>
		<column name="year" nullable="true" xsi:type="varchar" comment="year" length="255"/>
		<column name="make" nullable="true" xsi:type="varchar" comment="make" length="255"/>
		<column name="model" nullable="true" xsi:type="varchar" comment="model" length="255"/>
		<column name="image" nullable="true" xsi:type="varchar" comment="image" length="255"/>
		<column name="created_at" nullable="true" xsi:type="timestamp" comment="created_at"/>
	</table>
</schema>
